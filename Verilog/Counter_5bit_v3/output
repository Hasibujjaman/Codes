#! /usr/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20221226-515-g35f344adf)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x148f0b4f0 .scope module, "counter_tb" "counter_tb" 2 2;
 .timescale -9 -9;
v0x148f1ba50_0 .var "clk", 0 0;
v0x148f1bb20_0 .net "q", 4 0, v0x148f1b7f0_0;  1 drivers
S_0x148f0b680 .scope module, "uut" "counter" 2 6, 3 1 0, S_0x148f0b4f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 5 "q";
v0x148f1b8d0_0 .net "clk", 0 0, v0x148f1ba50_0;  1 drivers
v0x148f1b980_0 .net "q", 4 0, v0x148f1b7f0_0;  alias, 1 drivers
S_0x148f0b810 .scope module, "uut" "register" 3 5, 3 11 0, S_0x148f0b680;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 5 "q_reg";
v0x148f0b9a0_0 .net "clk", 0 0, v0x148f1ba50_0;  alias, 1 drivers
v0x148f1b750_0 .var "d_reg", 4 0;
v0x148f1b7f0_0 .var "q_reg", 4 0;
E_0x148f071c0 .event posedge, v0x148f0b9a0_0;
    .scope S_0x148f0b810;
T_0 ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x148f1b750_0, 0, 5;
    %end;
    .thread T_0;
    .scope S_0x148f0b810;
T_1 ;
    %wait E_0x148f071c0;
    %load/vec4 v0x148f1b750_0;
    %assign/vec4 v0x148f1b7f0_0, 0;
    %load/vec4 v0x148f1b750_0;
    %addi 1, 0, 5;
    %assign/vec4 v0x148f1b750_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x148f0b4f0;
T_2 ;
    %load/vec4 v0x148f1ba50_0;
    %inv;
    %store/vec4 v0x148f1ba50_0, 0, 1;
    %delay 10, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x148f0b4f0;
T_3 ;
    %vpi_call 2 15 "$dumpfile", "counter_tb.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x148f0b4f0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x148f1ba50_0, 0;
    %delay 20, 0;
    %delay 20, 0;
    %delay 20, 0;
    %delay 20, 0;
    %delay 20, 0;
    %delay 20, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x148f0b4f0;
T_4 ;
    %vpi_call 2 28 "$monitor", "clk = %b, Count = %d ", v0x148f1ba50_0, v0x148f1bb20_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
