S_0x11fe0ea30 .scope module, "counter_tb" "counter_tb" 2 2;
 .timescale -9 -9;
v0x11fe0ee90_0 .var "clk", 0 0;
v0x11fe0ef20_0 .net "q", 4 0, v0x11fe0ec50_0;  1 drivers
S_0x10ff04950 .scope module, "uut" "counter" 2 6, 3 1 0, S_0x11fe0ea30;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 5 "q";
v0x11fe0ece0_0 .net "clk", 0 0, v0x11fe0ee90_0;  1 drivers
v0x11fe0ed70_0 .var "d", 4 0;
v0x11fe0ee00_0 .net "q", 4 0, v0x11fe0ec50_0;  alias, 1 drivers
S_0x10ff04ae0 .scope module, "uut" "register" 3 7, 3 20 0, S_0x10ff04950;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 5 "d_reg";
    .port_info 2 /OUTPUT 5 "q_reg";
v0x10ff04d10_0 .net "clk", 0 0, v0x11fe0ee90_0;  alias, 1 drivers
v0x11fe0ebc0_0 .net "d_reg", 4 0, v0x11fe0ed70_0;  1 drivers
v0x11fe0ec50_0 .var "q_reg", 4 0;
E_0x10ff04cd0 .event posedge, v0x10ff04d10_0;
    .scope S_0x10ff04ae0;
T_0 ;
    %wait E_0x10ff04cd0;
    %load/vec4 v0x11fe0ebc0_0;
    %assign/vec4 v0x11fe0ec50_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x10ff04950;
T_1 ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x11fe0ed70_0, 0, 5;
    %end;
    .thread T_1;
    .scope S_0x10ff04950;
T_2 ;
    %wait E_0x10ff04cd0;
    %load/vec4 v0x11fe0ed70_0;
    %addi 1, 0, 5;
    %assign/vec4 v0x11fe0ed70_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x11fe0ea30;
T_3 ;
    %load/vec4 v0x11fe0ee90_0;
    %inv;
    %store/vec4 v0x11fe0ee90_0, 0, 1;
    %delay 10, 0;
    %jmp T_3;
    .thread T_3;
    .scope S_0x11fe0ea30;
T_4 ;
    %vpi_call 2 15 "$dumpfile", "counter_tb.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x11fe0ea30 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x11fe0ee90_0, 0;
    %delay 20, 0;
    %delay 20, 0;
    %delay 20, 0;
    %delay 20, 0;
    %delay 20, 0;
    %delay 20, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x11fe0ea30;
T_5 ;
    %vpi_call 2 28 "$monitor", "clk = %b, Count = %d ", v0x11fe0ee90_0, v0x11fe0ef20_0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
